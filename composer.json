{
    "name": "phlib/ua-classifier",
    "description": "A PHP ua-parser device classification project, to provide functionality that was removed in ua-parser 2.0",
    "keywords": [
        "phlib",
        "ua-classifier",
        "ua-parser",
        "uap-php"
    ],
    "homepage": "https://github.com/phlib/ua-classifier",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Jan Luetke"
        },
        {
            "name": "Chris Minett"
        },
        {
            "name": "Jamie Straw"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "ua-parser/uap-php": "^3.5",
        "symfony/yaml": "^4.0",
        "symfony/console": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "UAClassifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UAClassifier\\Test\\": "tests",
            "UAClassifier\\Cli\\": "cli"
        }
    },
    "suggest": {
        "ua-parser/uap-php": "ua-classifier is designed to extend the results provided by ua-parser"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    }
}
